package javaaa;
import java .util.*;
public class Palindrome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc= new Scanner(System.in);
		String s= sc.nextLine();
		s=s.toLowerCase();
		int start=0;
		int end=s.length()-1;
		while(start<=end) {
			if(s.charAt(start)==s.charAt(end)) {
				start++;
				end--;
			}
		}
	System.out.println("true");
	}

}
