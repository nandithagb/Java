import java.util.Scanner;
import java.util.*;
public class Full_pyramid {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int j=0;j<3;j++)
		{
			for(int i=0;i<=j;i++)
			{
				System.out.print("*"+"\t");
			}
			System.out.println();
		}
		for(int i=2;i>=0;i--)
		{
			for (int j=2;j<=0;j--)
			{
				System.out.print(" *"+"\t");
			}
			System.out.println();
		}

	}

}
